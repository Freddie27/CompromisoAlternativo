<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @*Referencia stylesheet kitgob*@
    <link href="~/Content/Kitgob.css" rel="stylesheet" />
    <!-- Tipografías obtenidas con google fonts-->
    <link type="text/css" rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300,400">
    <link type="text/css" rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto+Slab:300,400">
</head>
<body>
    <header>
        <!-- Acá va el encabezado del sitio (navbar)-->
        <nav class="navbar navbar-light navbar-expand-lg">
            <div class="container">
                <a class="navbar-brand" href="/"><i class="fa fa-spinner fa-spin page-loading-icon"></i><img src="http://www.serviuloslagos.cl/Imagenes/Logo_Serviu10.jpg" /></a>
                <button class="navbar-toggler collapsed" type="button" data-toggle="collapse" data-target="#navbarLightExampleCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation"><span class="navbar-toggler-icon"></span></button>
                <div class="navbar-collapse collapse" id="navbarLightExampleCollapse">
                    <ul class="navbar-nav ml-auto">

                        <li class="nav-item"><a class="nav-link" href="#">Link</a></li>
                        @*@Html.ActionLink("Application name", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" }) *@
                        <li class="nav-behavior"><a class="nav-link text-uppercase text-underline" href="#">en</a></li>
                        <li class="nav-separator"></li>
                        <li class="nav-behavior"><a class="nav-link" href="#"><i class="cl cl-login"></i></a></li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <main>
        <!-- Acá va el contenido del sitio-->
        <div class="container">
            @RenderBody()
        </div>
    </main>
    <footer class="content-always-on-display">
        <div class="container">
            <div class="line"></div>

        </div>
    </footer>

    <!-- JS-->
    <script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js" integrity="sha384-9/reFTGAW83EW2RDu2S0VKaIzap3H66lZH81PoYlFhbGU+6BZp6G7niu735Sk7lN" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.min.js" integrity="sha384-w1Q4orYjBQndcko6MimVbzY0tgp4pWB4lZ7lr30WKz0vr/aWKhXdBNmNb5D92v7s" crossorigin="anonymous"></script>
    <script rel="stylesheet" src="http://cdn.digital.gob.cl/framework/js/gob.cl.js"></script>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/complementos")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
